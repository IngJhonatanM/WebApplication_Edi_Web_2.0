@{
    ViewData["Title"] = "Contact";
}
<h1>@ViewData["Modulo De Contacto"]</h1>

<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Información de Contacto</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.3/font/bootstrap-icons.css">

    <style>
        .contacto {
            text-align: center;
            margin-top: 50px;
        }
    </style>
</head>
<body>
    <!-- contact form start -->
    <section class="section contact-info pb-0">
        <div class="container">
            <div class="row">
                <div class="col-lg-4 col-md-6">
                    <div class="card-header text-center border-4 mb-4 mb-lg-0">
                        <i class="bi bi-headset" style="font-size: 6rem; color: dark;"></i>
                        <h5>Nuestro Número</h5>
                        +58 (212) 709 5600
                    </div>
                </div>
                <div class="col-lg-4 col-md-6">
                    <div class="card-header text-center border-4 mb-4 mb-lg-0">
                        <i class="bi bi-envelope-exclamation" style="font-size: 6rem; color: dark;"></i>
                        <h5>Nuestro Email</h5>
                        info@eniac.com​
                    </div>
                </div>
                <div class="col-lg-4 col-md-12">
                    <div class="card-header text-center border-4 mb-4 mb-lg-0">
                        <i class="bi bi-pin-map" style="font-size: 5rem; color: dark;"></i>
                        <h5>Ubicación</h5>
                        Av. Principal de La Castellana, Torre La Castellana, Piso 4, Chacao. Caracas, Venezuela
                    </div>
                </div>
            </div>
        </div>
    </section>

    <section class="contact-form-wrap section">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-6">
                    <div class="section-title text-center">
                        <br>
                        <h2 class="text-md mb-2">Contáctanos</h2> 
                        <hr/>
                        <div class="divider mx-auto my-4"></div>
                        <p class="mb-5"> 
                        Nuestro equipo está listo para responder sus preguntas. ¡Nos encantaría saber sobre usted!
                        </p>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
    </section>
</body>
</html>
